<div class="page-container">
    <div class="container">
        <div class="row g-4">
            {{#each articles}}
            <div class="col-lg-4 col-md-6">
                <div class="bill-card">
                    <h2>{{this.title}}</h2>
                    <p><strong>Article URL:</strong> {{this.url}}</p>
                    <p><strong>Category:</strong> {{this.category}}</p>
                    <p class="amount-due">Source: ${{this.source}}</p>
                    <p><strong>Published on:</strong> {{this.publishedDate}}</p>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>
